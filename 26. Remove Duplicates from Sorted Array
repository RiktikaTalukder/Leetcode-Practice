class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
     

        # 'l' points to the position of the last unique element
        l = 0

        # Start 'r' from 1 and compare with nums[l]
        for r in range(1, len(nums)):
            # When a new unique value is found
            if nums[r] != nums[l]:
                l += 1
                nums[l] = nums[r]  # Move unique value forward

        # Number of unique values = l + 1
        return l + 1
